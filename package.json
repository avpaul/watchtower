{
  "name": "watch-tower",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/__mocks__/",
      "!src/registerServiceWorker.js",
      "!src/**/index.js",
      "!src/redux/**"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "lodash.mapvalues": "^4.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**' -c .eslintrc.json",
    "galen": "galen",
    "pretest:galen": "pm2 start -n watch-tower ./node_modules/react-scripts/scripts/start.js",
    "posttest:galen": "pm2 stop watch-tower",
    "test:galen": "galen test ./galen/tests/ --htmlreport ./galen/reports/"
  },
  "devDependencies": {
    "chromedriver": "^2.42.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "moxios": "^0.4.0",
    "react-test-renderer": "^16.5.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "galenframework": "^2.3.7",
    "geckodriver": "^1.12.2",
    "pm2": "^3.2.2"
  }
}
